{
  "name": "poker-marketplace-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "LTC",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 nest build",
    "build:legacy": "cross-env NODE_OPTIONS=--max_old_space_size=8192 NODE_OPTIONS=--openssl-legacy-provider nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=8192 NODE_ENV=production nest start",
    "start:legacy": "cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_OPTIONS=--max_old_space_size=8192 NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_OPTIONS=--max_old_space_size=8192 NODE_ENV=local nest start --watch",
    "start:dev:legacy": "cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_OPTIONS=--max_old_space_size=8192 NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start-auth:debug": "nest start auth --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/marketplace/test/jest-e2e.json",
    "docker:env:up": "docker-compose -f docker-compose.development.yml --env-file .env.develop up",
    "docker:env:down": "docker-compose -f docker-compose.development.yml --env-file .env.develop down",
    "docker:stop": "docker stop $(docker ps -q)",
    "cards-cli": "ts-node -r tsconfig-paths/register apps/cards-cli/src/main.ts",
    "client": "cd client&&ts-node index.ts",
    "services:up": "docker-compose -f ./docker-compose.services.yml --env-file .env.local up -d",
    "services:down": "docker-compose -f ./docker-compose.services.yml --env-file .env.local down",
    "ipfs": "node ./ipfs.util.js"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "1.0.0",
    "@nestjs/axios": "0.0.8",
    "@nestjs/common": "8.4.5",
    "@nestjs/core": "8.4.5",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/mongoose": "9.0.3",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.4.5",
    "@nestjs/schedule": "1.1.0",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/typeorm": "8.0.3",
    "@types/json2csv": "5.0.3",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "axios": "0.27.2",
    "bignumber.js": "^9.1.0",
    "copy-webpack-plugin": "10.2.4",
    "cross-env": "7.0.3",
    "dotenv": "16.0.1",
    "env-var": "7.1.1",
    "express": "4.18.1",
    "ioredis": "5.0.4",
    "ipfs-core-types": "0.10.3",
    "ipfs-http-client": "56.0.3",
    "json2csv": "5.0.7",
    "mongodb": "4.6.0",
    "mongoose": "6.3.3",
    "nest-logger": "7.0.0",
    "nestjs-console": "7.0.1",
    "node-localstorage": "2.2.1",
    "ntc": "0.0.1",
    "passport": "0.5.3",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "path": "0.12.7",
    "pg": "8.7.3",
    "redis": "4.1.0",
    "reflect-metadata": "0.1.13",
    "request": "2.88.2",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "svg-parser": "2.0.4",
    "swagger-ui-express": "4.4.0",
    "tronweb": "4.2.0",
    "typeorm": "0.3.6",
    "web3": "^1.7.5"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.5",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.5",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.1",
    "@types/node": "17.0.33",
    "@types/node-localstorage": "1.3.0",
    "@types/request": "2.48.8",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.24.0",
    "@typescript-eslint/parser": "5.24.0",
    "docker-compose": "0.23.17",
    "eslint": "8.15.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "source-map-support": "0.5.21",
    "supertest": "6.2.3",
    "ts-jest": "28.0.2",
    "ts-loader": "9.3.0",
    "ts-node": "10.7.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/domain(|/.*)$": "<rootDir>/libs/domain/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/contracts(|/.*)$": "<rootDir>/libs/contracts/src/$1",
      "^@app/fslog(|/.*)$": "<rootDir>/libs/fslog/src/$1",
      "^@app/blockchain(|/.*)$": "<rootDir>/libs/blockchain/src/$1"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache",
      "bash -c tsc"
    ]
  }
}
